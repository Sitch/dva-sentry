{
  "name": "dva-sentry",
  "version": "0.1.0",
  "description": "report crash in dva",
  "main": "./lib/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf src/*.js && rm -rf lib",
    "compile:ts": "tsc",
    "compile:babel": "babel src --out-dir lib",
    "compile": "npm run compile:ts && npm run compile:babel",
    "dev": "concurrently 'npm run compile:ts -- -w --sourcemap' 'npm run compile:babel -- -w'",
    "lint": "tslint \"src/*.ts\"",
    "pub": "npm run clean && npm run compile && npm publish"
  },
  "keywords": [
    "dva",
    "sentry"
  ],
  "author": "jaredleechn@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "concurrently": "^3.4.0",
    "tslint": "^4.5.1"
  },
  "dependencies": {
    "raven-js": "^3.12.1",
    "typescript": "^2.2.1"
  },
  "peerDependencies": {
    "dva": "^1.1.0"
  },
  "files": [
    "lib"
  ],
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "add-module-exports",
      "transform-runtime"
    ]
  }
}
